{
  "name":"client-app-inner-page",
  "version":"1.0.0",
  "component": true,
  "description":"Inner page for JS-KIT",
  "author":"Tony Mobily",
  "moduleDependencies":[
    "client-app-frame"
  ],
  "position": -1,
  "manipulate":{
    "text":{
      "src/pages/<%=vars.newElementFullName%>.js":[
        {
          "op":"resolve-ejs"
        },
      ],
      // TODO
      "<%=vars.newElementInfo.parentElementPath%>":[
        {
          // TODO
          "op": "insert",
          "position": "after",
          "newlineAfter": true,
          "newlineBefore": false,
          "anchorPoint": "<%-vars.newElementInfo.destination.anchorPoint%>",
          "value":"<<%=vars.newElementInfo.name%> slot=\"content\"></<%=vars.newElementInfo.name%>>"
        },
        {
          // TODO
          "op": "insert",
          "position": "before",
          "newlineAfter": false,
          "anchorPoint": "/* Loaded modules -- end */",
          "value": "import '<%=vars.newElementInfo.importPath%>'"
        },
        {
          // TO CHECK
          "op": "insert",
          "position": "after",
          "newlineAfter": true,
          "newlineBefore": false,
          "anchorPoint": '<!-- Page tab heading insertion point -->',
          "value":"<a name=\"<%=vars.newElementInfo.nameNoPrefix%>\" href=\"${window.location.pathname}<%=vars.newElementInfo.subPath\"><div><%=vars.newElementInfo.title%>%></div></a>"
        }
      ]
    }
    }
  }
}
