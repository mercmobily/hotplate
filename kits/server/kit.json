{
   "name":"server",
   "version":"1.0.0",
   "description":"js-kit base server with stores support",
   "author":"Tony Mobily",
   "kitDependencies":[
   ],
   "manipulateJson": {
      "package.json": {
        "set":{
           "scripts.start":"SERVE_BUILT=true NODE_ENV=production node server/www",
           "scripts.start:dev":"node server/www",
           "scripts.start:dev:debug":"node --inspect-brk server/www ",
           "scripts.start:dev:built":"SERVE_BUILT=true node server/www",
           "dependencies.DEP1":"^3.1.0",
           "devDependencies.DEVDEP1":"^4.1.0"
        },
        "unset":[
           "scripts.start:build"
        ]
      }
   },
   "packageJson":{
      "set":{
         "scripts.start":"SERVE_BUILT=true NODE_ENV=production node server/www",
         "scripts.start:dev":"node server/www",
         "scripts.start:dev:debug":"node --inspect-brk server/www ",
         "scripts.start:dev:built":"SERVE_BUILT=true node server/www",
         "dependencies.DEP1":"^3.1.0",
         "devDependencies.DEVDEP1":"^4.1.0"
      },
      "unset":[
         "scripts.start:build"
      ]
   },
   "postinstall":"cd server; npm install"
}
